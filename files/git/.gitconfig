[user]
	name = Dennis Leon
	email = dennis.leon39@gmail.com
[core]
	autocrlf = input
 excludesfile = /opt/boxen/config/git/gitignore
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
[alias]
	ci = commit
	st = status
    lg-local-commits = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit  origin/master..HEAD
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	g = grep --break --heading --line-number
[grep]
	extendRegexp = true
	lineNumber = true
[push]
	default = matching

[hub]
protocol = https

[credential]
helper = /opt/boxen/bin/boxen-git-credential
